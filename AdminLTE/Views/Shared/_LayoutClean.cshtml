
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    @Html.RenderStyles(StyleFiles.Bootstrap, StyleFiles.FontAwesome, StyleFiles.Ionicons, StyleFiles.ICheck, StyleFiles.Style)
    @RenderSection("styles", false)

    <!--[if lt IE 9]>
        @Html.RenderScripts(ScriptFiles.Html5shiv, ScriptFiles.Respond)
    <![endif]-->

</head>
<body>
    <div class="container body-content">
        @RenderBody()
    </div>
</body>
@Html.RenderScripts(ScriptFiles.Jquery, ScriptFiles.Bootstrap, ScriptFiles.Layer, ScriptFiles.ICheck, ScriptFiles.Common)
@RenderSection("scripts", required: false)
</html>
